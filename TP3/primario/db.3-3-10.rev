;
; BIND reverse data file for local loopback interface
;

$TTL   604800
@      IN     SOA dns.cc.pt grupo02@cc.pt(
		1        ; Serial
		604800   ; Refresh
		86400    ; Retry
		2419200  ; Expire
		604800 ) ; Negative Cache TTL
;

@       IN    NS   Serv1.cc.pt.
@	IN    NS   Serv2.cc.pt.

1	IN    PTR  Serv1.cc.pt.
2 	IN    PTR  Serv2.cc.pt.
3	IN    PTR  Serv3.cc.pt.
3  	IN    PTR  mail.cc.pt.
2  	IN    PTR  mail2.cc.pt.
3	IN    PTR  www.cc.pt.

 
